{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    },
    {
      "src": "/api/auth",
      "dest": "/api/auth.ts"
    },
    {
      "src": "/api/callback",
      "dest": "/api/callback.ts"
    }
  ]
}
